{
  "name": "persocloud",
  "version": "1.0.1",
  "license": "MIT",
  "description": "La comparaison de données à portée de Cloud",
  "repository": {
    "type": "git",
    "url": "https://gitlab.irisa.fr/pcloud/app-cozy.git"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "ng build --base-href /apps/persocloud/ --target=development --watch true --environment=dev",
    "watch:dev": "npm run watch",
    "watch:dev_api": "ng build --base-href /apps/persocloud/ --target=development --watch true --environment=dev_api",
    "watch:prod": "ng build --base-href /apps/persocloud/ --target=production --watch true --environment=prod",
    "build": "ng build --base-href /apps/persocloud/ --target=development --environment=dev",
    "build:dev": "npm run build",
    "build:dev_api": "ng build --base-href /apps/persocloud/ --target=development --environment=dev_api",
    "build:prod": "ng build --base-href /apps/persocloud/ --target=production --environment=prod",
    "serve": "ng serve --target=development --environment=dev",
    "serve:dev": "npm run serve",
    "serve:dev_api": "ng serve --target=development --environment=dev_api",
    "serve:prod": "ng serve --target=production --environment=prod"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "angular2-fontawesome": "^0.9.0",
    "angular2-moment": "^1.3.3",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "icheck": "^1.0.2",
    "ion-rangeslider": "^2.1.7",
    "jquery-mask-plugin": "^1.14.10",
    "ng2-charts": "^1.5.0",
    "ng2-ion-range-slider": "^1.0.1",
    "ngx-bootstrap": "^1.6.6",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "^2.4.0",
    "@types/node": "~6.0.60",
    "tslint": "^4.0.0",
    "ts-node": "~2.0.0",
    "typescript": "~2.0.0"
  }
}
